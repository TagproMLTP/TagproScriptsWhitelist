{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Tagpro.me Sync Agent",
  "short_name": "TagProMeSync",
  "description": "tagpro.me sync extension",
  "version": "1.3.0",
  "manifest_version": 2,
  "permissions": [],
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "content_scripts": [
    {
      "matches": ["http://*.koalabeast.com/*", "http://*.jukejuice.com/*", "http://tagpro.me/*"],
      "js": ["main.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "js/catstats.js"
  ]
}
